import pandas as pd
import numpy as np
import matplotlib.pyplot as plt
import seaborn as sns
df = pd.read_csv("Sleep_health_and_lifestyle_dataset.csv")
print(df.head(10))
print(df.tail(10))
print(df.info)
print(df.isna().sum())
print(df.dropna())
print(df.duplicated())
print(df.drop_duplicates())
print(df.describe())
df["Daily Steps"].unique()
print(["Daily Steps"])
plt.figure(figsize=(8, 5))
df['Daily Steps'].hist(bins=20, edgecolor='black')
plt.title('Daily Steps')
plt.xlabel('')
plt.ylabel('')
plt.show()
plt.figure(figsize=(10, 5))
plt.plot(df['Heart Rate'], df['Stress Level'], marker='o', linestyle='-', color='b')
plt.title('')
plt.xlabel('Stress Level')
plt.ylabel('Heart Rate')
plt.xticks(rotation=45)
plt.grid(True)
plt.show()
plt.figure(figsize=(8, 5))
df['Blood Pressure'].value_counts().plot(kind='bar', color='skyblue')
plt.title('')
plt.xlabel('Blood Pressure')
plt.ylabel('')
plt.show()
plt.figure(figsize=(8, 5))
sns.boxplot(data=df[['Stress Level', 'Heart Rate', 'Blood Pressure']])
plt.title('')
plt.ylabel('')
plt.show()
plt.figure(figsize=(8, 6))
corr = df[['Sleep Duration', 'Age', 'Stress Level']].corr()
sns.heatmap(corr, annot=True, cmap='coolwarm', fmt='.2f')
plt.title('')
plt.show()
grouped_mean = df.groupby('Gender')[['Age', 'Stress Level', 'Heart Rate']].mean()
print("")
print(grouped_mean)
grouped_count = df.groupby('BMI Category').size()
print("")
print(grouped_count)
grouped_sum = df.groupby('Gender')['Physical Activity Level'].sum()
print("")
print(grouped_sum)
pivot_table = pd.pivot_table(df, values='Age', index='Quality of Sleep', aggfunc=['mean', 'count', 'sum'])
print("")
print(pivot_table)
